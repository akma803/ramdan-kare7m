local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "😈cop hub😈", HidePremium = false, SaveConfig = true, ConfigFolder = "مطور السكربت marwan and adam"})


local Tab = Window:MakeTab({
	Name = "سكربتات كل شيء",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "the important script"
})


OrionLib:MakeNotification({
	Name = "hello bro 🗿!",
	Content = "Notification content... منور ياغالي",
	Image = "rbxassetid://4483345998",
	Time = 5
})

Tab:AddButton({
	Name = "السكربت تحت صيانه 🗿😮‍💨! ",
	Callback = function()
      		print("button pressed")
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]
